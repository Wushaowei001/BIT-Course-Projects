ul
{
	margin:0;	
	padding:0;
}

li
{
	float:left;
	list-style:none;	
}

.navigator/*导航*/
{
	height:40px;
	width:1366px;
	margin-top:-8px;
	margin-left:-8px;
	margin-right:-8px;
}

.navigator li/*导航每个部件*/
{
	height:40px;
	width:100px;
	text-align:center;
	line-height:40px;
	color:white;
	list-style:none;
	float:left;
	display:block;
	background:#404144;
}

.navigator #checked/*被选中的页面*/
{
	background:#333333;	
	
}

.navigator li:hover/*鼠标悬浮*/
{
	background:#333333;
	cursor:pointer;	
}

